{
  "name": "eln-plugin",
  "version": "0.0.1",
  "description": "Extract metainfo from chemical file formats into a well defined json structure",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fake-require": "node bin/require-types.js",
    "clean": "rm -f src/req.js",
    "browserify": "mkdir -p dist && browserify src/index.js --require=./src/req.js --standalone=elnPlugin > dist/eln-plugin.js",
    "build": "npm run fake-require && npm run browserify && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/eln-plugin.git"
  },
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/eln-plugin/issues"
  },
  "homepage": "https://github.com/cheminfo/eln-plugin#readme",
  "devDependencies": {
    "browserify": "^13.0.0"
  }
}
